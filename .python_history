import RPi.GPIO as GPIO
GPIO.setmode(GPIO.BCM)
GPIO.setup(27, GPIO.OUT)
GPIO.gpio_function(27)
tst = GPIO.gpio_function(27)
print(tst)
GPIO.gpio_function(22)
GPIO.gpio_function(2)
GPIO.gpio_function(3)
GPIO.gpio_function(4)
GPIO.gpio_function(14)
GPIO.gpio_function(23)
GPIO.gpio_function(24)
GPIO.gpio_function(17)
GPIO.gpio_function(18)
GPIO.output(27, 1)
GPIO.output(27, 0)
GPIO.output(24, 0)
