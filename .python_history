for i in range(0, 200):
import time
for i in range(0, 200):
    print(i)
for i in range(0, 200):
    time.sleep(1)
    print(GPIO.input(2))
GPIO.setup(2, GPIO.IN, GPIO.PUD_OFF)
for i in range(0, 200):
    time.sleep(0.5)
    print(GPIO.input(2))
GPIO.setup(2, GPIO.IN, GPIO.PUD_DOWN)
for i in range(0, 200):
    time.sleep(0.1)
    print(GPIO.input(2))
GPIO.setup(2, GPIO.IN, GPIO.PUD_UP)
for i in range(0, 200):
    time.sleep(0.1)
    print(GPIO.input(2))
GPIO.setup(2, GPIO.IN, GPIO.PUD_DOWN)
print(GPIO.input(2))
GPIO.setup(3, GPIO.IN, GPIO.PUD_DOWN)
print(GPIO.input(3))
GPIO.setup(4, GPIO.IN, GPIO.PUD_DOWN)
print(GPIO.input(4))
print(GPIO.input(2))
GPIO.setup(14, GPIO.IN, GPIO.PUD_DOWN)
print(GPIO.input(14))
GPIO.setup(2, GPIO.IN, pull_up_down=GPIO.PUD_DOWN)
print(GPIO.input(2))
GPIO.setup(7, GPIO.IN, pull_up_down=GPIO.PUD_DOWN)
print(GPIO.input(7))
print(GPIO.input(2))
GPIO.setup(2, GPIO.IN, pull_up_down=GPIO.PUD_UP)
print(GPIO.input(2))
GPIO.cleanup()
exit()
